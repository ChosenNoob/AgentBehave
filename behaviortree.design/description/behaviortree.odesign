<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:filter="http://www.eclipse.org/sirius/diagram/description/filter/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:validation="http://www.eclipse.org/sirius/description/validation/1.1.0" name="behaviortree" version="12.0.0.2017041100">
  <ownedViewpoints name="behaviortree" modelFileExtension="behaviortree">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="Tree Representation" initialisation="true" domainClass="behaviortree::BehaviorTree" enablePopupBars="true">
      <metamodel href="http://www.example.org/behaviortree#/"/>
      <filters xsi:type="filter:CompositeFilterDescription" name="TreeSkeletonCollapse" label="Fold Tree Skeletons">
        <filters xsi:type="filter:MappingFilter" mappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Action%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Condition%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Fallback%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Sequence%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Tree%20Skeleton']" semanticConditionExpression="aql:self.eContainer().eClass() &lt;>  behaviortree::TreeSkeleton"/>
      </filters>
      <validationSet>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="Leaf Node Check" level="ERROR" message="Trees leaf nodes should be either of the type Action or Condition Node" targetClass="behaviortree::BehaviorTree">
          <audits auditExpression="service:checkLeafNodes()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="Debugger" message="service:debugger()" targetClass="behaviortree::BehaviorTree">
          <audits auditExpression="aql:false"/>
        </ownedRules>
      </validationSet>
      <diagramInitialisation>
        <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.createNode(behaviortree::EntryPoint)">
          <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="Entry Point"/>
          <subModelOperations xsi:type="tool:CreateInstance" typeName="behaviortree::EntryPoint" referenceName="entryPoint" variableName="entryPointInstance"/>
        </firstModelOperations>
      </diagramInitialisation>
      <defaultLayer name="Default">
        <nodeMappings name="Action Node" domainClass="behaviortree::ActionNode">
          <style xsi:type="style:SquareDescription" labelSize="12" tooltipExpression="aql:self.name" sizeComputationExpression="aql:self.name.size()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Condition Node" domainClass="behaviortree::ConditionNode">
          <style xsi:type="style:SquareDescription" labelSize="12" tooltipExpression="aql:self.name" sizeComputationExpression="aql:self.name.size()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Sequence Node" domainClass="behaviortree::SequenceNode">
          <style xsi:type="style:SquareDescription" labelSize="12" tooltipExpression="aql:self.name" sizeComputationExpression="aql:self.name.size()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Root" domainClass="behaviortree::Root">
          <style xsi:type="style:SquareDescription" labelSize="12" tooltipExpression="aql:self.name" sizeComputationExpression="aql:self.name.size()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Fallback Node" domainClass="behaviortree::FallbackNode">
          <style xsi:type="style:SquareDescription" labelSize="12" tooltipExpression="aql:self.name" sizeComputationExpression="aql:self.name.size()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Tree Skeleton" domainClass="behaviortree::TreeSkeleton">
          <style xsi:type="style:SquareDescription" labelSize="12" tooltipExpression="aql:self.name+' contains '+self.eAllContents()->size()+' nodes.'" sizeComputationExpression="aql:self.name.size()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='purple']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="Children" sourceMapping="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Fallback%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Root'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Sequence%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Tree%20Skeleton']" targetMapping="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Action%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Condition%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Fallback%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Sequence%20Node'] //@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Tree%20Skeleton']" targetFinderExpression="aql:self.children">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="Create Nodes">
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="ActionNodeCreation" label="Create a Action Node" nodeMappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Action%20Node']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.createNode(behaviortree::ActionNode)">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="aql:'Action Node '+container.eAllContents()->filter(behaviortree::ActionNode)->size()"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="ConditionNodeCreation" label="Create a Condition Node" nodeMappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Condition%20Node']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.createNode(behaviortree::ConditionNode)">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="aql:'Condition Node '+container.eAllContents()->filter(behaviortree::ConditionNode)->size()"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="SequenceNodeCreation" label="Create a Sequence Node" nodeMappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Sequence%20Node']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.createNode(behaviortree::SequenceNode)">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="aql:'Sequence Node '+container.eAllContents()->filter(behaviortree::SequenceNode)->size()"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="FallbackNodeCreation" label="Create a Fallback Node" nodeMappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Fallback%20Node']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.createNode(behaviortree::FallbackNode)">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="aql:'Fallback Node '+container.eAllContents()->filter(behaviortree::FallbackNode)->size()"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="Tools">
          <ownedTools xsi:type="tool_1:NodeCreationDescription" documentation="Create an Entry Point (There can only exist one, it should generate when you create the viewpoint)&#xD;&#xA;" name="EntryPointCreation" label="Create an Entry Point" precondition="aql:container.entryPoint = null" nodeMappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Entry%20Point']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.createNode(behaviortree::EntryPoint)">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="Entry Point"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="TreeSkeletonCreation" label="Create a Tree Skeleton" forceRefresh="true" inverseSelectionOrder="true" nodeMappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@nodeMappings[name='Tree%20Skeleton']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.createNode(behaviortree::EntryPoint)">
                <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="aql:'Tree Skeleton '+container.eAllContents()->filter(behaviortree::TreeSkeleton)->size()"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="Connect Nodes">
          <ownedTools xsi:type="tool_1:EdgeCreationDescription" documentation="First, select the node that is going to be parent, then select the node that is going to be children&#xD;&#xA;" name="Set Children" precondition="service:canBeChild(preTarget)" edgeMappings="//@ownedViewpoints[name='behaviortree']/@ownedRepresentations[name='Tree%20Representation']/@defaultLayer/@edgeMappings[name='Children']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.setChild(target)"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="behaviortree.design.Services"/>
  </ownedViewpoints>
</description:Group>
